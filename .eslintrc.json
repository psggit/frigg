{
    "extends": "airbnb",
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "indent": [2],
      "react/prefer-stateless-function": [0, {"ignorePureComponents": true}],
      "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
      "class-methods-use-this": [0],
      "no-trailing-spaces": 0,
      "react/sort-comp": 0,
      "jsx-a11y/anchor-is-valid": 0
    }
}
